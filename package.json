{
  "name": "rsbuild-vue3-storybook",
  "version": "0.0.0",
  "repository": "https://github.com/rspack-contrib/rsbuild-vue3-storybook",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.js",
      "require": "./dist/preset.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist/**/*", "README.md", "*.js", "*.d.ts", "!src/**/*"],
  "scripts": {
    "build": "tsup ./src/index.ts ./src/preset.ts --format esm,cjs --dts --cjsInterop --clean",
    "dev": "tsup ./src/index.ts ./src/preset.ts --format esm,cjs --dts --cjsInterop --clean --watch",
    "lint": "biome check .",
    "lint:write": "biome check . --write",
    "prepare": "simple-git-hooks",
    "test": "playwright test",
    "bump": "npx bumpp"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint:write"
  },
  "dependencies": {
    "@rollup/pluginutils": "~5.1.3",
    "@storybook/vue3": "^8.4.7",
    "find-package-json": "~1.2.0",
    "magic-string": "~0.30.15",
    "storybook-builder-rsbuild": "~0.1.6",
    "vue-component-meta": "~2.1.10",
    "vue-docgen-api": "~4.79.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@playwright/test": "^1.49.1",
    "@rsbuild/core": "^1.1.10",
    "@types/find-package-json": "^1.2.6",
    "@types/node": "^22.10.2",
    "playwright": "^1.49.1",
    "simple-git-hooks": "^2.11.1",
    "storybook": "^8.4.7",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@rsbuild/core": "1.x"
  },
  "peerDependenciesMeta": {
    "@rsbuild/core": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.15.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
